{
  "manifest_version": 3,
  "name": "Discontent",
  "version": "1.0",
  "description": "Decorate links that are garbage",
  "icons": {
    "48": "icons/green_heart-48.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_icon": "icons/green_heart-48.png",
    "default_title": "Discontent",
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "include_globs": [
        "*://*.google.*/*",
        "*://*.bing.com/*",
        "*://duckduckgo.com/*"
      ],
      "js": [
        "content_scripts/index.js"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "background_scripts/index.js"
  },
  "key": ""
}
